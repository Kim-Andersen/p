{
  "name": "server",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "prestart": "npm run db:migrate && npm run db:seed:all",
    "start": "node build/server.js",
    "dev": "NODE_ENV=development concurrently \"npm run build:watch\" \"npm run serve:watch\"",
    "serve:watch": "nodemon build/server.js",
    "build": "tsc --build",
    "build:watch": "npm run build -- --watch --incremental",
    "test": "jest --config ../.config/jest.config.json",
    "test:coverage": "npm run test -- --coverage",
    "db:seed": "sequelize-cli db:seed:all",
    "db:migrate": "sequelize-cli db:migrate --config build/config/database.js",
    "db:seed:all": "sequelize-cli db:seed:all --config build/config/database.js"
  },
  "devDependencies": {
    "@types/jest": "26.0.4",
    "@types/koa": "2.11.3",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-json": "2.0.18",
    "@types/koa-logger": "3.1.1",
    "@types/koa-mount": "4.0.0",
    "@types/koa-router": "7.4.1",
    "@types/node": "14.0.23",
    "@types/pg": "7.14.4",
    "@types/sequelize": "4.28.9",
    "@types/validator": "13.1.0",
    "concurrently": "5.2.0",
    "jest": "26.1.0",
    "jest-junit": "11.0.1",
    "nodemon": "2.0.4",
    "ts-jest": "26.1.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.6"
  },
  "dependencies": {
    "fakerator": "0.3.1",
    "koa": "2.13.0",
    "koa-bodyparser": "4.3.0",
    "koa-json": "2.0.2",
    "koa-logger": "3.2.1",
    "koa-mount": "4.0.0",
    "koa-router": "9.1.0",
    "pg": "8.3.0",
    "pg-hstore": "2.3.3",
    "sequelize": "6.3.3",
    "sequelize-cli": "6.2.0"
  }
}
